<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="style.css">
  <title>Document</title>
</head>
<body>
  <table>
    <thead>
      <tr>
        <th>User ID</th>
        <th>Name</th>
        <th>Role</th>
        <th>Password</th>
      </tr>
    </thead>
    <tbody>
      <% users.map(user => { %>
        <tr>
          <td><%= user.userID %></td>
          <td><%= user.name %></td>
          <td><%= user.role %></td>
          <td><%= user.password %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  
  
  
  
</body>
</html>